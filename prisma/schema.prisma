// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("LOCAL_MONGO_DATABASE")
}

model Users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  name     String
  password String
  email    String @unique

  @@map("users")
}
